{
  "realm": "quarkus-realm",
  "enabled": true,
  "users": [
    {
      "username": "admin",
      "enabled": true,
      "email": "admin@example.com",
      "emailVerified": true,
      "firstName": "Alice",
      "lastName": "Admin",
      "credentials": [
        {
          "type": "password",
          "value": "test"
        }
      ],
      "realmRoles": ["admin", "user"]
    },
    {
      "username": "test",
      "enabled": true,
      "email": "test@example.com",
      "emailVerified": true,
      "firstName": "Bob",
      "lastName": "Tester",
      "credentials": [
        {
          "type": "password",
          "value": "test"
        }
      ],
      "realmRoles": ["user"]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "admin",
        "description": "Administrator role"
      },
      {
        "name": "user",
        "description": "Standard user role"
      }
    ]
  },
  "clients": [
    {
      "clientId": "quarkus-client",
      "secret": "testsecret",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": false,
      "directAccessGrantsEnabled": true,
      "standardFlowEnabled": true,
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": true,
      "clientAuthenticatorType": "client-secret",
      "redirectUris": ["*"],
      "webOrigins": ["*"],
      "defaultRoles": ["user"],
      "attributes": {
        "access.token.lifespan": "3600",
        "client.secret.creation.time": "0"
      }
    }
  ]
}
